        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
	var EBcurrency = current.estimated_budget.getCurrencyCode(); // get currency of estimated budget
	var RBcurrency = current.real_budget.getCurrencyCode(); // currency of real budget

	if (RBcurrency != EBcurrency) { // if currencies are different, send error
		current.setAbortAction(true);
		gs.addErrorMessage(
			gs.getMessage(
				'Cannot save project "{0}" because currency of estimated budget and real budget are not the same!', [
				current.getValue('name')
			]));
	}
})(current, previous);




]]></script>
