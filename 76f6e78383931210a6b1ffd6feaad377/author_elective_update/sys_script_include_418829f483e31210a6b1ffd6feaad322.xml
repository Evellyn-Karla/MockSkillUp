        <script><![CDATA[var GetCourseType = Class.create();
GetCourseType.prototype = {
    initialize: function () {
    },

    getType: function() {
        var grCourse = new GlideRecord('x_1588110_mock_s_0_course');
        grCourse.addQuery('type', current.variables.course_type); 
        grCourse.query();  

        var courses = [];
        while (grCourse.next()) {
            courses.push(grCourse.getValue('sys_id'));  
        }

        return courses;  
    },

    type: 'GetCourseType' 
};
]]></script>
